{
  "compilerOptions": {
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@@/*": [".dumi/tmp/*"],
      "react-draggable-plus": ["src"],
      "react-draggable-plus/*": ["src/*", "*"]
    }
  },
  "exclude": ["node_modules"],
  "include": [".dumirc.ts", "src/**/*"]
}
